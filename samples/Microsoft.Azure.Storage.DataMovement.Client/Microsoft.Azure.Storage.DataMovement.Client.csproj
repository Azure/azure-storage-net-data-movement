<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputPath>bin\</OutputPath>
		<OutputType>Exe</OutputType>
		<IsTestProject>false</IsTestProject>
		<TargetFrameworks>net462;netcoreapp3.1</TargetFrameworks>
		<LangVersion>9</LangVersion>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="CommandLineParser" Version="2.8.0" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'netcoreapp3.1'">
		<ProjectReference Include="..\..\netcore\Microsoft.Azure.Storage.DataMovement\Microsoft.Azure.Storage.DataMovement.csproj" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net462'">
		<ProjectReference Include="..\..\lib\DataMovement.csproj" />

		<PackageReference Include="Microsoft.Azure.KeyVault.Core" Version="1.0.0.0" />
		<PackageReference Include="Microsoft.Azure.Storage.Blob" Version="11.2.3.0" />
		<PackageReference Include="Microsoft.Azure.Storage.Common" Version="11.2.3.0" />
		<PackageReference Include="Microsoft.Azure.Storage.File" Version="11.2.3.0" />
		<PackageReference Include="System.Net.Http" Version="4.3.4" />
	</ItemGroup>


</Project>